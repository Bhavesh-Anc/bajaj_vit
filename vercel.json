{
  "version": 2,
  "name": "bfhl-api-vit",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/bfhl",
      "methods": ["GET", "POST"],
      "dest": "index.js"
    },
    {
      "src": "/health",
      "methods": ["GET"],
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "index.js": {
      "maxDuration": 10
    }
  },
  "regions": ["bom1", "sin1"],
  "github": {
    "silent": true
  }
}
